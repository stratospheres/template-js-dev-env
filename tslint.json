{
	"extends": "tslint:recommended",
	"rules": {
			"max-line-length": {
					"options": [120]
			},
			"new-parens": true,
			"no-arg": true,
			"no-bitwise": true,
			"no-conditional-assignment": true,
			"no-consecutive-blank-lines": false,
			"quotemark": [true, "double"],
			"indent": [true, "tabs", 2],
			"no-var-requires": false,
			"trailing-comma": false,
			"no-reference": false,
			"no-console": false
	},
	"jsRules": {
			"max-line-length": {
					"options": [120]
			}
	}
}
